<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sdaproject.SearchCarUI">
   <children>
      <ImageView fx:id="SChome" fitHeight="90.0" fitWidth="663.0" layoutX="-1.0" onMouseClicked="#home" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@homepage.png" />
         </image>
      </ImageView>
      <TextField fx:id="SearchBar" layoutX="40.0" layoutY="98.0" prefHeight="25.0" prefWidth="551.0" promptText="Search" />
      <AnchorPane layoutX="25.0" layoutY="172.0" prefHeight="298.0" prefWidth="614.0">
         <children>
            <ListView fx:id="List1" prefHeight="253.0" prefWidth="619.0" />
         </children>
      </AnchorPane>
      <Button fx:id="SearchBT" layoutX="593.0" layoutY="93.0" mnemonicParsing="false" onAction="#SearchBTClick" text="Submit" />
      <TextField fx:id="Min" layoutX="104.0" layoutY="136.0" promptText="Minimum Price" />
      <TextField fx:id="Max" layoutX="294.0" layoutY="136.0" promptText="Maximum Price" />
      <Button fx:id="filterBT" layoutX="493.0" layoutY="133.0" mnemonicParsing="false" onAction="#filter" text="Filter" />
   </children>
</AnchorPane>
